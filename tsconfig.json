{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "strict": false,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true,
        "noImplicitReturns": false,
        "noImplicitAny": false,
        "module": "es6",
        "moduleResolution": "node",
        "target": "es5",
        "allowJs": true,
        "checkJs": false
    },
    "exclude": ["./src/scorplib/*"],
    "include": ["./src/**/*"]
}
